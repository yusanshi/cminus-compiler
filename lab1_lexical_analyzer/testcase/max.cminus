/* first comment ** */
int max(int x, int y, int z)
{
/*** find the max of three numbers*** ** * /
(this multi-line comment is intended to test comment matching. *****/
	if (x > y)
	{
		if (x > z)
		{
			return x;
		}
		else
		{
			return z;
		}
	}
	else
	{
		if (y > z)
		{
			return y;
		}
		else
		{
			return z;
		}
	}
}

/**** multiple comments *****/ 

int main()
{
	int x;
	int y;
	int z;
	x = 5;
	y = 6;
	z = 1;
	max(x, y, z);
	return 0;
}
