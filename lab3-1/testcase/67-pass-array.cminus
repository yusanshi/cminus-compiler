int add(int a[], int length) {
    int i;
    i = 0;
    int sum;
    sum = 0;
    while (i < length) {
        sum = sum + a[i];
        i = i + 1;
    }
    return sum;
}
int main(void) {
    int a[10];
    int i;
    i = 0;
    while (i < 10) {
        a[i] = input();
    }
    return add(a, 10);
}